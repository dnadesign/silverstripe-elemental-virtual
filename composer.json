{
    "name": "dnadesign/silverstripe-elemental-virtual",
    "description": "Adds ability for Elemental to share content blocks between pages",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": "2.x-dev",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "elemental-virtual",
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "css",
            "images"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ElementalVirtual\\": "src/",
            "SilverStripe\\ElementalVirtual\\Tests\\": "tests/php/"
        }
    }
}
